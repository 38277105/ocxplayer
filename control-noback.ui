QSlider::groove:horizontal {
     border: 1px solid #999999;
     height:2px;
    /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
     background: grey;
     
 }
QSlider::handle:horizontal {
     background: rgb(52,52,52);
     border: 1px solid #5c5c5c;
     width:10px;
     height:16px;
     
     margin: -8px 0px;  /*handle is placed by default on the contents rect of the groove. Expand outside the groove */
     border-radius: 1px;
 }
 QSlider::add-page:horizontal {
     background: grey;
 }

 QSlider::sub-page:horizontal {
     background: rgb(0, 137, 156);
     height:3px;
 }



